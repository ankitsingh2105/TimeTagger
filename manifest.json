{
    "manifest_version": 3,
    "name": "YouTube Custom Timestamps",
    "version": "1.0",
    "description": "Add custom timestamps with colors to YouTube videos.",
    "permissions": ["storage"],
    "host_permissions": ["https://www.youtube.com/*"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/watch*"],
        "js": ["content.js"]
      }
    ]
  }
  